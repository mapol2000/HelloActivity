<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MPActivity">

    <ImageView
        android:id="@+id/iv1"
        android:layout_width="126dp"
        android:layout_height="157dp"
        app:layout_constraintEnd_toStartOf="@+id/iv2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv2"
        app:srcCompat="@drawable/pic1" />

    <ImageView
        android:id="@+id/iv2"
        android:layout_width="126dp"
        android:layout_height="157dp"
        app:layout_constraintEnd_toStartOf="@+id/iv3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv1"
        app:layout_constraintTop_toTopOf="@+id/iv3"
        app:srcCompat="@drawable/pic2" />

    <ImageView
        android:id="@+id/iv3"
        android:layout_width="126dp"
        android:layout_height="157dp"
        app:layout_constraintBottom_toTopOf="@+id/iv6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="spread"
        app:srcCompat="@drawable/pic3" />

    <ImageView
        android:id="@+id/iv4"
        android:layout_width="126dp"
        android:layout_height="157dp"
        app:layout_constraintEnd_toStartOf="@+id/iv5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv5"
        app:srcCompat="@drawable/pic4" />

    <ImageView
        android:id="@+id/iv5"
        android:layout_width="126dp"
        android:layout_height="157dp"
        app:layout_constraintEnd_toStartOf="@+id/iv6"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv4"
        app:layout_constraintTop_toTopOf="@+id/iv6"
        app:srcCompat="@drawable/pic5" />

    <ImageView
        android:id="@+id/iv6"
        android:layout_width="126dp"
        android:layout_height="157dp"
        app:layout_constraintBottom_toTopOf="@+id/iv9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv5"
        app:layout_constraintTop_toBottomOf="@+id/iv3"
        app:srcCompat="@drawable/pic6" />

    <ImageView
        android:id="@+id/iv7"
        android:layout_width="126dp"
        android:layout_height="157dp"
        app:layout_constraintEnd_toStartOf="@+id/iv8"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/iv8"
        app:srcCompat="@drawable/pic7" />

    <ImageView
        android:id="@+id/iv8"
        android:layout_width="126dp"
        android:layout_height="157dp"
        app:layout_constraintEnd_toStartOf="@+id/iv9"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv7"
        app:layout_constraintTop_toTopOf="@+id/iv9"
        app:srcCompat="@drawable/pic8" />

    <ImageView
        android:id="@+id/iv9"
        android:layout_width="126dp"
        android:layout_height="157dp"
        app:layout_constraintBottom_toTopOf="@+id/btnResult"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/iv8"
        app:layout_constraintTop_toBottomOf="@+id/iv6"
        app:srcCompat="@drawable/pic9" />

    <Button
        android:id="@+id/btnResult"
        android:layout_width="388dp"
        android:layout_height="48dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="22dp"
        android:backgroundTint="#B3ADAD"
        android:onClick="goResult"
        android:text="투표종료"
        app:iconTint="#7C7777"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#FFFFFF" />
</androidx.constraintlayout.widget.ConstraintLayout>