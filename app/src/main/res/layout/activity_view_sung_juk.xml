<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewSungJukActivity">

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="146dp"
        android:layout_marginEnd="18dp"
        android:text="이름 :"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/tvName1"
        app:layout_constraintEnd_toStartOf="@+id/tvName1"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tvTot1"
        android:layout_width="83dp"
        android:layout_height="22dp"
        android:textAlignment="textStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView8"
        app:layout_constraintStart_toStartOf="@+id/tvMat1" />

    <TextView
        android:id="@+id/tvMat1"
        android:layout_width="83dp"
        android:layout_height="22dp"
        android:textAlignment="textStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="@+id/tvEng1" />

    <TextView
        android:id="@+id/tvKor1"
        android:layout_width="83dp"
        android:layout_height="22dp"
        android:textAlignment="textStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView5"
        app:layout_constraintStart_toStartOf="@+id/tvName1" />

    <TextView
        android:id="@+id/tvAvg1"
        android:layout_width="83dp"
        android:layout_height="22dp"
        android:textAlignment="textStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="@+id/tvTot1" />

    <TextView
        android:id="@+id/tvEng1"
        android:layout_width="83dp"
        android:layout_height="22dp"
        android:textAlignment="textStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView6"
        app:layout_constraintStart_toStartOf="@+id/tvKor1" />

    <TextView
        android:id="@+id/tvGrd1"
        android:layout_width="83dp"
        android:layout_height="22dp"
        android:textAlignment="textStart"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/textView10"
        app:layout_constraintStart_toStartOf="@+id/tvAvg1" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
